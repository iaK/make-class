{
    "name": "iak/make-class",
    "description": "A package to create basic classes with tests in your Laravel app",
    "keywords": [
        "iaK",
        "laravel",
        "make-class"
    ],
    "homepage": "https://github.com/iak/make-class",
    "license": "MIT",
    "authors": [
        {
            "name": "Isak Berglind",
            "email": "info@isakberglind.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Iak\\MakeClass\\": "src",
            "Iak\\MakeClass\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iak\\MakeClass\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iak\\MakeClass\\MakeClassServiceProvider"
            ],
            "aliases": {
                "MakeClass": "Iak\\MakeClass\\MakeClassFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
